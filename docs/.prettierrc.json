{
  "semi": false,
  "singleQuote": true,
  "jsxSingleQuote": true,
  "trailingComma": "none",
  "embeddedLanguageFormatting": "off",
  "plugins": ["@prettier/plugin-php"],
  "overrides": [
    {
      "files": "*.md",
      "options": {
        "proseWrap": "preserve"
      }
    },
    {
      "files": "*.mdx",
      "options": {
        "parser": "mdx",
        "proseWrap": "preserve"
      }
    }
  ]
}
